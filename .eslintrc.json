{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-await-in-loop": 0,
        "no-plusplus": 0,
        "react/jsx-props-no-spreading": 0,
        "max-len": 1,
        "react/jsx-first-prop-new-line": 2,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}
